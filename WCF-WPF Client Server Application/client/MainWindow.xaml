<Window x:Class="client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="subs_button" Content="Subscribe" HorizontalAlignment="Left" Height="22" Margin="110,32,0,0" VerticalAlignment="Top" Width="320" Click="button_Click"/>
        <TextBlock x:Name="time_block" x:FieldModifier="public" HorizontalAlignment="Left" Height="34" Margin="110,250,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="320"/>
        <Image x:Name="clock_image" HorizontalAlignment="Left" Height="137" Margin="181,79,0,0" VerticalAlignment="Top" Width="173"/>

    </Grid>
</Window>
